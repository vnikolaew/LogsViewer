{
   "Logging": {
	  "LogLevel": {
		 "Default": "Information",
		 "Microsoft.AspNetCore": "Warning"
	  }
   },
   "LogConfigurations": {
	  "DataCaptureClient": {
		 "ServiceName": "DataCaptureClient",
		 "LogFormat": "{date:date}\\s{time:time}\\s\\[{process_id:int}-{thread_id:int}\\]\\s{log_level:logLevel}\\s|\\s{content:string}"
	  },
	  "OcrClient": {
		 "ServiceName": "OcrClient",
		 "LogFormat": "{date:date}\\s{time:time}\\s\\[{process_id:int}-{thread_id:int}\\]\\s{log_level:logLevel}\\s|\\s{content:string}"
	  }
   }
}
